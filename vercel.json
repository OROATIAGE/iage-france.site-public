{
  "rewrites": [
    {
      "source": "/((?!.*\\.pdf$).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/documents/(.*)\\.pdf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Content-Disposition",
          "value": "inline"
        }
      ]
    }
  ]
} 
